@page
@using SeguiAcademico.App.Dominio
@model SeguiAcademico.App.Presentacion.Pages.Tutores.EliminarModel
@{
}
<h1>Menú eliminación de tutores</h1>
<form method="POST">

    <div class="form-group">
        <label asp-for="tutor.Id"></label>
        <input asp-for="tutor.Id" class="form-control" readonly>
    </div>
    <div class="form-group">
        <label asp-for="tutor.Nombre"></label>
        <input asp-for="tutor.Nombre" class="form-control" readonly>
    </div>
    <div class="form-group">
        <label asp-for="tutor.Apellidos"></label>
        <input asp-for="tutor.Apellidos" class="form-control" readonly>
    </div>
    

    <button type="submit" class="btn btn-primary">Eliminar</button>


</form>