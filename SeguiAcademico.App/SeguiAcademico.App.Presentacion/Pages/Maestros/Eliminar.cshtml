@page
@using SeguiAcademico.App.Dominio
@model SeguiAcademico.App.Presentacion.Pages.Maestros.EliminarModel
@{
}
<h1>Menú eliminación de maestros</h1>
<form method="post">

    <div class="form-group">
        <label asp-for="maestro.Id"></label>
        <input asp-for="maestro.Id" class="form-control" readonly>
    </div>
    <div class="form-group">
        <label asp-for="maestro.Nombre"></label>
        <input asp-for="maestro.Nombre" class="form-control" readonly>
    </div>
    <div class="form-group">
        <label asp-for="maestro.Apellidos"></label>
        <input asp-for="maestro.Apellidos" class="form-control" readonly>
    </div>
    

    <button type="submit" class="btn btn-primary">Eliminar</button>


</form>