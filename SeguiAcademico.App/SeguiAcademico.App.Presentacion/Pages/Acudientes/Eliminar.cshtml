@page
@model SeguiAcademico.App.Presentacion.Pages.Acudientes.EliminarModel
@{
}
<h1>Menú eliminación de acudientes</h1>
<form method="POST">

    <div class="form-group">
        <label asp-for="acudiente.Id"></label>
        <input asp-for="acudiente.Id" class="form-control" readonly>
    </div>
    <div class="form-group">
        <label asp-for="acudiente.Nombre"></label>
        <input asp-for="acudiente.Nombre" class="form-control" readonly>
    </div>
    <div class="form-group">
        <label asp-for="acudiente.Apellidos"></label>
        <input asp-for="acudiente.Apellidos" class="form-control" readonly>
    </div>
    

    <button type="submit" class="btn btn-primary">Eliminar</button>


</form>